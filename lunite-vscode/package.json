{
  "name": "lunite",
  "displayName": "Lunite Language",
  "description": "Lunite language support for VS Code",
  "version": "0.0.1",
  "publisher": "lunite-lang",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "lunite",
        "aliases": ["Lunite", "lunite"],
        "extensions": [".lun"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "lunite",
        "scopeName": "source.lunite",
        "path": "./syntaxes/lunite.tmLanguage.json"
      }
    ]
  },
  "activationEvents": [
    "onLanguage:lunite"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "^16.x",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "vscode-languageclient": "^8.1.0"
  }
}
