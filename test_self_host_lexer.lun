import "std/lexer.lun" as lex
import "std/token.lun" as tok

fn main() {
    let source = "struct Test";
    print("Tokenizing source:");
    print(source);
    
    // Gọi hàm static của struct thông qua Module Alias
    let l = lex.Lexer.new(source);
    let t1 = l.next_token();
    
    // In ra để kiểm tra
    print("First token line:");
    lunite_print_int(t1.line);
    
    print("SUCCESS: Generics and Module Alias are working!");
}
