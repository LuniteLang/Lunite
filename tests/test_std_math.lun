import "std/math.lun" as math;
extern fn lunite_print_int(i: int) -> void
extern fn print(s: string) -> void

fn main() -> int {
    let p = math.pow(2.0, 3.0);
    print("2^3 = ");
    lunite_print_int(p as int); // cast float to int for printing
    print("\n");

    let c = math.ceil(2.3);
    print("ceil(2.3) = ");
    lunite_print_int(c as int);
    print("\n");

    let f = math.floor(2.7);
    print("floor(2.7) = ");
    lunite_print_int(f as int);
    print("\n");
    
    return 0;
}
