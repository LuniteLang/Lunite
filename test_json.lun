import "std/json.lun" as json
import "std/string.lun" as str

fn main() {
    let s = "{\"name\": \"Lunite\", \"version\": 1}";
    let val = json.parse(s);
    
    when (val) {
        json.JsonValue::Object(obj) => {
            str.println("Parsed Object Successfully!");
        }
        else => {
            str.println("Parse Failed or Not an Object");
        }
    }
}
